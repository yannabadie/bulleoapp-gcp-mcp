# Configuration Firebase
FIREBASE_PROJECT_ID=bulleoapp-prod
FIREBASE_STORAGE_BUCKET=bulleoapp-prod.appspot.com
FIREBASE_DATABASE_URL=https://bulleoapp-prod.firebaseio.com

# Configuration GCP
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json
GCP_PROJECT_ID=bulleoapp-prod
GCP_REGION=europe-west1

# API Keys (si nécessaire)
GEMINI_API_KEY=your-gemini-api-key

# Healthcare API (pour conformité HDS)
HEALTHCARE_DATASET_ID=bulleoapp-healthcare
FHIR_STORE_ID=bulleoapp-fhir

# Environnement
NODE_ENV=production
DEBUG=false

# Sécurité
ENCRYPTION_KEY=your-256-bit-encryption-key
JWT_SECRET=your-jwt-secret

# Intégrations externes
DOCTOLIB_API_KEY=your-doctolib-key
DOCTOLIB_API_SECRET=your-doctolib-secret